<div class="page-header">
  <h1>Artículos</h1>
</div>
<a href="/articles/new" class="btn btn-default pull-right">Agregar artículo</a>
<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Codigo</th>
      <th>Cantidad</th>
    </tr>
  </thead>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.name %></td>
        <td><%= article.code %></td>
        <td><%= article.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
